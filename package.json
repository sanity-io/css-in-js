{
  "name": "css-in-js-root",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "format": "prettier --write .",
    "lint": "oxlint --type-aware",
    "build": "pnpm -r build",
    "postinstall": "pnpm -r generateErrors",
    "test": "pnpm -r test"
  },
  "dependencies": {
    "@types/react": "19.1.11",
    "@types/react-dom": "19.1.8",
    "typescript": "5.9.2"
  },
  "devDependencies": {
    "@sanity/prettier-config": "^2.0.1",
    "oxlint": "^1.13.0",
    "oxlint-tsgolint": "^0.0.4",
    "prettier": "^3.6.2"
  },
  "prettier": "@sanity/prettier-config",
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "react",
        "react-dom",
        "react-is"
      ]
    },
    "overrides": {
      "@types/react": "$@types/react",
      "@types/react-dom": "$@types/react-dom"
    }
  }
}

{
  "name": "css-in-js-root",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "postinstall": "pnpm -r generateErrors",
    "test": "pnpm -r test"
  },
  "dependencies": {
    "@types/react": "19.1.7",
    "@types/react-dom": "19.1.6",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.12.1",
  "engines": {
    "node": ">=20.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowAny": [
        "react",
        "react-dom",
        "react-is"
      ]
    },
    "overrides": {
      "@types/react": "$@types/react",
      "@types/react-dom": "$@types/react-dom"
    },
    "patchedDependencies": {
      "@vitejs/plugin-react": "patches/@vitejs__plugin-react.patch"
    }
  }
}
